			public function guardarTotalLiquidacion(avaluo:Avaluo, myUsuario:Usuario):LiquidacionAvaluoTotal{
				try{ 
					liquidacionAvaluoTotalVO.avaluo = avaluo ; 
					
					if(avaluo.liquidacionTotal != null && avaluo.liquidacionTotal.length > 0){
						var lTotal:LiquidacionAvaluoTotal = avaluo.liquidacionTotal.getItemAt(0) as LiquidacionAvaluoTotal ; 
						if(lTotal != null){
							liquidacionAvaluoTotalVO.idLiqavaluoTotal = lTotal.idLiqavaluoTotal ; 
							liquidacionAvaluoTotalVO.fechaCreacion = lTotal.fechaCreacion ; 
							liquidacionAvaluoTotalVO.usuarioCreacion = lTotal.usuarioCreacion ;
						}					
					}
					
					if(cbxCalificacion.selectedIndex > -1) {
						liquidacionAvaluoTotalVO.calificacion = cbxCalificacion.selectedItem.rvLowValue;
					}
					if((totalAvaluoTextInput.text != null) && (totalAvaluoTextInput.text.length > 0)) {
						liquidacionAvaluoTotalVO.totalAvaluo = totalAvaluoTextInput.text;
					}
					if((valorUvrDiaTextInput.text != null) && (valorUvrDiaTextInput.text.length > 0)) {
						liquidacionAvaluoTotalVO.valorUvrDia = valorUvrDiaTextInput.text;
					}
					if((avaluoUvrTextInput.text != null) && (avaluoUvrTextInput.text.length > 0)) {
						liquidacionAvaluoTotalVO.avaluoUvr = avaluoUvrTextInput.text;
					}
					if((valorAsegurableTextInput.text != null) && (valorAsegurableTextInput.text.length > 0)) {
						liquidacionAvaluoTotalVO.valorAsegurable = valorAsegurableTextInput.text;
					}
					liquidacionAvaluoTotalVO.usuarioCreacion = myUsuario.codigoUsuario;
					liquidacionAvaluoTotalVO.usuarioTransaccion = myUsuario.codigoUsuario;
				}catch(error:Error){
					error.getStackTrace(); 
					trace(error.message); 
					parentDocument.imprimirEstadoProceso(error.message);
				}
				return liquidacionAvaluoTotalVO ; 
			}
			
