			private function formatearMoneda(valor:String):String {
				
				var currencyFormatter:CurrencyFormatter = new CurrencyFormatter();
				var texto:String = valor;								
				try{
					if (valor != null && valor.length > 0)
					{
						if (int(valor) > 0)
						{	
							texto = currencyFormatter.format(valor);							
						}		
					}
				}	
				catch(e:Error)
				{
					trace(e);
				}	
				return texto;	
			}
			
